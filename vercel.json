{
  "rewrites": [{ "source": "/(.*)", "destination": "/" }],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "Content-Language", "value": "es" },
        { "key": "X-Frame-Options", "value": "DENY" }
      ]
    }
  ]
}